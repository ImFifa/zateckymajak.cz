{
	"name": "filipurban.cz/zateckymajak.cz",
	"description": "zateckymajak.cz",
	"type": "project",
	"license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    { "type": "git", "url": "git@github.com:K2D-CZ/core.git" },
    { "type": "git", "url": "git@github.com:K2D-CZ/box.git" },
	{ "type": "git", "url": "git@github.com:K2D-CZ/file.git" },
	{ "type": "git", "url": "git@github.com:K2D-CZ/gallery.git" },
	{ "type": "git", "url": "git@github.com:K2D-CZ/analytics.git" }
  ],
  "require": {
    "php": ">= 7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "ext-xmlwriter": "*",
    "nette/nette": "^3.0.0",
    "orisai/nette-di": "^1.0.0",
    "simple-cms/box-module": "v2.0.0",
    "simple-cms/core-module": "v2.0.0",
	"simple-cms/file-module": "v2.0.0",
	"simple-cms/gallery-module": "v2.0.0",
	"simple-cms/analytics-module": "v2.0.0",
    "symfony/dotenv": "^5.1",
    "tracy/tracy": "^2.7.6"
  },
  "require-dev": {
    "orisai/coding-standard": "^1.0.0",
    "phpstan/extension-installer": "^1.0.0",
    "phpstan/phpstan": "^0.12.0",
    "phpstan/phpstan-deprecation-rules": "^0.12.0",
    "phpstan/phpstan-nette": "^0.12.9",
    "phpstan/phpstan-strict-rules": "^0.12.0",
    "staabm/annotate-pull-request-from-checkstyle": "^1.3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.4"
    }
  }
}
